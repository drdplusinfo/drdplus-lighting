{
  "name": "drd-plus/lighting",
  "license": "MIT",
  "type": "library",
  "description": "Effects of insufficient lighting or to bright light for Drd+",
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "require": {
    "php": ">=7.0",
    "roave/security-advisories": "dev-master",
    "granam/strict-object": "dev-master",
    "granam/integer": "dev-master",
    "drd-plus/calculations": "dev-master",
    "drd-plus/tables": "dev-master",
    "drd-plus/rolls-on": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "~0.9"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
